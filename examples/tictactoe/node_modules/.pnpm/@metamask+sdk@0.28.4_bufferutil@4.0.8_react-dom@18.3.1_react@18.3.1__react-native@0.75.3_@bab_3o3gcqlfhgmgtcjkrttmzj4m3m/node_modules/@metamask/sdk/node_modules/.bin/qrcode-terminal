#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/qrcode-terminal-nooctal@0.12.1/node_modules/qrcode-terminal-nooctal/bin/node_modules:/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/qrcode-terminal-nooctal@0.12.1/node_modules/qrcode-terminal-nooctal/node_modules:/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/qrcode-terminal-nooctal@0.12.1/node_modules:/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/qrcode-terminal-nooctal@0.12.1/node_modules/qrcode-terminal-nooctal/bin/node_modules:/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/qrcode-terminal-nooctal@0.12.1/node_modules/qrcode-terminal-nooctal/node_modules:/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/qrcode-terminal-nooctal@0.12.1/node_modules:/Users/hamdiallam/workspace/op-supersim/examples/tictactoe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../qrcode-terminal-nooctal/bin/qrcode-terminal.js" "$@"
else
  exec node  "$basedir/../../../../qrcode-terminal-nooctal/bin/qrcode-terminal.js" "$@"
fi
